	uninstall-mp4mLTLIBRARIES uninstall-mp4msharedDATA

uninstall-man: uninstall-man3

.PHONY: CTAGS GTAGS all all-am check check-am clean clean-generic \
	clean-libtool clean-mp4mLTLIBRARIES ctags distclean \
	distclean-compile distclean-generic distclean-libtool \
	distclean-tags distdir dvi dvi-am html html-am info info-am \
	install install-am install-data install-data-am \
	install-data-hook install-exec install-exec-am install-info \
	install-info-am install-man install-man3 \
	install-mp4mLTLIBRARIES install-mp4msharedDATA install-strip \
	installcheck installcheck-am installdirs maintainer-clean \
	maintainer-clean-generic mostlyclean mostlyclean-compile \
	mostlyclean-generic mostlyclean-libtool pdf pdf-am ps ps-am \
	tags uninstall uninstall-am uninstall-info-am uninstall-man \
	uninstall-man3 uninstall-mp4mLTLIBRARIES \
	uninstall-mp4msharedDATA


.src.mp4hp:
	sed -e '/^=head1/,/^=cut/d' -e '/^##EOF##/,$$d' $< > $@
	cat $< > $(thisdir):$*.pod && pod2man --section=$(man3ext) \
   --center="mp4h modules" \
   --release="mp4h modules" \
   $(thisdir):$*.pod > $(thisdir)$*.$(man3ext) && rm -f $(thisdir):$*.pod

$(man3_MANS): $(mp4mshared_DATA)

install-data-hook:
	list='$(man3_MANS)'; for f in $${list}; do \
      mv $(DESTDIR)$(man3dir)/$$f $(DESTDIR)$(man3dir)/`echo $$f | sed -e 's/^$(thisdir)/$(thisdir):/'`; \
    done
# Tell versions [3.59,3.63) of GNU make to not export all variables.
# Otherwise a system limit (for SysV at least) may be exceeded.
.NOEXPORT:
